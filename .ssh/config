    User aosman
    Hostname cn01
    proxyJump tosun
    LocalForward 2222 localhost:8888

Host tosun_cn_away
    User aosman
    Hostname cn05
    proxyJump tosun_away
    LocalForward 2222 localhost:8888

Host tosun_away_forward
    User aosman
    Hostname 10.3.5.102
    proxyJump aliosmanberk@flow.sabanciuniv.edu
    LocalForward 2222 localhost:8888
